<DayGrid.Root className={classes.Root}>
  <tbody>
    {weeks.map((week) => (
      <DayGrid.Row key={week[0].toString()} className={classes.Row}>
        {week.map((day) => (
          <DayGrid.Cell key={day.toString()} className={classes.Cell}>
            <span className={classes.CellLabel}>{day.toFormat('dd')}</span>
          </DayGrid.Cell>
        ))}
      </DayGrid.Row>
    ))}
  </tbody>
</DayGrid.Root>